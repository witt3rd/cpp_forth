mem 98 + 1 .

0 while dup 98 < do
  0 while dup 100 < do
    dup mem + , if
      mem 100 + 42 .
    else
      mem 100 + 32 .
    end
    1 mem 100 + 1 1 syscall3

    1 +
  end
  drop

  mem 100 + 10 .
  101 mem 100 + 1 1 syscall3

  // pattern =
  mem     , 1 shl // mem[0] << 1
  mem 1 + ,       // mem[1]
  bor             // mem[0] | mem[1]

  1 while dup 98 < do           // index
     swap 1 shl 7 band          // ((pattern << 1) & 7)
     over mem + 1 + , bor       // | (mem[index + 1]) = new_pattern
     2dup 110 swap shr 1 band   // ((110 >> new_pattern) & 1)
     swap mem + swap .
     swap

     1 +
  end
  drop
  drop

  1 +
end
drop
